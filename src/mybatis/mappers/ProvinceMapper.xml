<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mapper.ProvinceMapper">
	
	<select id="getProvinceById" parameterType="java.lang.Integer" resultType="com.bean.ProvinceBean">
		select * from provinces where id = #{id}
	</select>
	
	<select id="getProvinceList" resultType="com.bean.ProvinceBean">
		select * from provinces
	</select>
	
</mapper>